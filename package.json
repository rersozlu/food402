{
  "name": "food402",
  "version": "1.1.6",
  "description": "MCP server for ordering food from TGO Yemek",
  "type": "module",
  "bin": {
    "food402": "./dist/src/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc",
    "postinstall": "node dist/src/postinstall.js",
    "prepublishOnly": "npm run build",
    "test": "vitest run --config test/vitest.config.ts",
    "test:watch": "vitest --config test/vitest.config.ts",
    "test:coverage": "vitest run --config test/vitest.config.ts --coverage",
    "test:security": "vitest run --config test/vitest.config.ts test/security/"
  },
  "keywords": [
    "mcp",
    "food",
    "tgo-yemek",
    "claude",
    "ai",
    "food-ordering"
  ],
  "author": "rafiers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rersozlu/food402"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^17.2.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
"vitest": "^4.0.18"
  }
}
